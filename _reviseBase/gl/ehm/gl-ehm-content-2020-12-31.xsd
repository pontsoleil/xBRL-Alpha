	<import namespace="http://www.xbrl.org/2003/instance" schemaLocation="http://www.xbrl.org/2003/xbrl-instance-2003-12-31.xsd"/>
	<include schemaLocation="gl-ehm-2020-12-31.xsd"/>
	<complexType name="serialLotComplexType">
		<complexContent>
			<restriction base="anyType">
				<sequence>
					<element ref="gl-ehm:serialLotCode" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotDescription" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotNumber" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:nextSerialLotNumber" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotQuantity" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotOriginalQuantity" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotRemainingQuantity" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotOrigination" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotExpiration" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotManufacturer" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotBatchDescription" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotWarrantyStartDate" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotWarrantyEndDate" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotWarrantyPeriod" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotWarrantyPeriodUnit" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotWarrantyVendor" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotWarrantyContract" minOccurs="0" maxOccurs="1"/>
					<element ref="gl-ehm:serialLotComment" minOccurs="0" maxOccurs="1"/>
				</sequence>
				<attribute name="id" type="ID"/>
			</restriction>
		</complexContent>
	</complexType>
	<!-- tuple -->
	<element name="serialLot" id="gl-ehm_serialLot" type="gl-ehm:serialLotComplexType" substitutionGroup="xbrli:tuple" nillable="true"/>
</schema>
