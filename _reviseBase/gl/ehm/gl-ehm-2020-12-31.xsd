			<restriction base="xbrli:stringItemType">
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="serialLotDescriptionItemType">
		<simpleContent>
			<restriction base="xbrli:stringItemType">
			</restriction>
		</simpleContent>
	</complexType>
	<complexType name="serialLotNumberItemType">
